<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewStation">
   <children>
      <TextField fx:id="Fare" layoutX="151.0" layoutY="119.0" />
      <Label layoutX="61.0" layoutY="123.0" text="Fare:" />
      <Label layoutX="61.0" layoutY="183.0" text="Nearest Intersection:" />
      <TextField fx:id="NearestIntersection" editable="false" layoutX="200.0" layoutY="179.0" />
      <Label layoutX="87.0" layoutY="285.0" text="When checked, passengers can enter at this station" />
      <Button fx:id="update" layoutX="332.0" layoutY="119.0" mnemonicParsing="false" onAction="#Update" text="Update" underline="true" />
      <Label fx:id="StationName" layoutX="60.0" layoutY="56.0" text="Station Name">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Button fx:id="refresh" layoutX="274.0" layoutY="327.0" mnemonicParsing="false" onAction="#refresh" text="Refresh" />
      <CheckBox fx:id="OpenStationChecked" layoutX="80.0" layoutY="255.0" mnemonicParsing="false" text="Open Status" />
   </children>
</AnchorPane>
